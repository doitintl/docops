{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gloss: No config",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: No config (verbose)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["--verbose", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: No config (more verbose)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["--more-verbose", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: No config (max verbose)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["--max-verbose", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: Test config",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["-c", "tests/config", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: Test config (quiet)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["-c", "tests/config", "-q", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "Gloss: Test config (verbose)",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gloss",
      "args": ["-c", "tests/config", "-v", "tests/examples"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    },
    {
      "name": "GitBook: Test auth",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/docops-gitbook",
      "args": ["--whoami"],
      "env": {
        "DOCOPS_DEBUG": "1"
      },
      "console": "integratedTerminal"
    }
  ]
}
