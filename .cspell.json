{
  "version": "0.2",
  "language": "en-US",
  "caseSensitive": false,
  "allowCompoundWords": true,
  "minWordLength": 2,
  "ignorePaths": ["poetry.lock"],
  "dictionaries": [
    "custom-abbr",
    "custom-python",
    "custom-misc",
    "custom-terms"
  ],
  "dictionaryDefinitions": [
    {"name": "custom-abbr", "path": ".cspell.d/abbr.txt"},
    {"name": "custom-python", "path": ".cspell.d/python.txt"},
    {"name": "custom-misc", "path": ".cspell.d/misc.txt"},
    {"name": "custom-terms", "path": ".cspell.d/terms.txt"}
  ]
}
